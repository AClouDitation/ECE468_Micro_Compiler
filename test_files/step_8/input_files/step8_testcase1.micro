PROGRAM loop1
BEGIN
    FUNCTION VOID main ()
    BEGIN
        STRING newline := "\n";
        INT a, b, c, d;
        READ(a,b,c,d);
--        FOR (a:=0; a<b; a:=a+1)
        a := 0;
        WHILE (a < b)
            c := b;
            d := c+4;
            a := a+1;
        ENDWHILE
        WRITE (a,newline,b,newline,c,newline,d,newline);
    END
END
